<p id="notice"><%= notice %></p>

<h1>Education Credentials</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>University</th>
      <th>Percentage</th>
      <th>Course type</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @education_credentials.each do |education_credential| %>
      <tr>
        <td><%= education_credential.course %></td>
        <td><%= education_credential.university %></td>
        <td><%= education_credential.percentage %></td>
        <td><%= education_credential.course_type %></td>
        <td><%= education_credential.user %></td>
        <td><%= link_to 'Show', education_credential %></td>
        <td><%= link_to 'Edit', edit_education_credential_path(education_credential) %></td>
        <td><%= link_to 'Destroy', education_credential, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education Credential', new_education_credential_path %>
